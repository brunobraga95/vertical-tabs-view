{
  "name": "Vertical Tabs View",
  "version": "0.0.3.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Vertical Tabs View",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage","tabs"],
  "icons": { 
    "16": "verticaltabsview16.png",
    "48": "verticaltabsview48.png",
   "128": "verticaltabsview128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E",
        "windows": "Ctrl+I",
        "linux": "Ctrl+Q"
      },
      "description": "Opens the extension popup."
    }
  }
}



